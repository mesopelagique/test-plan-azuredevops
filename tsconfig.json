{
    "compilerOptions": {
        "module": "es6",
        "moduleResolution": "node",
        "target": "es6",
        "noImplicitAny": false,
        "jsx": "react",
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitThis": true,
        "lib": [
            "es6",
            "es2015.promise",
            "dom",
            "dom.iterable"
        ],
        "rootDir": "scripts",
        "outDir": "dist"
    }
}